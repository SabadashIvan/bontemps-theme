<div class="" data-section-id="{{ section.id }}" data-section-type="quotes">
<div class="contained">
<h1>{{ section.settings.section_title }}</h1>
<p>{{ section.settings.subtitle }}</p>
</div>

  {% if section.blocks.size == 0 %}
    {% include 'no-blocks' %}
  {% endif %}
</div>

{% schema %}
  {
    "name": {
      "en": "About main Content"
    },
    "class": "index-section",
    "settings": [
      {
        "type": "text",
        "id": "section_title",
        "label": {
          "en": "Section Heading"
        },
        "default": {
          "en": "Rethinking beverages for our female contemporaries."
        }
      },
      {
        "type": "text",
        "id": "subtitle",
        "label": {
          "en": "Section subeading"
        },
        "default": {
          "en": "Our blends are designed to heighten the highs, cushion the lows and complement the in-betweens of women’s everyday pursuits."
        }
      }
    ],
    "blocks": [
      {
        "type": "quote",
        "name": "About block",
        "settings": [
          {
            "type": "text",
            "id": "heading",
            "label": "section heading",
            "default": {
              "en": "Rethinking beverages for our female contemporaries."
            }
          },
          {
            "type": "text",
            "id": "author",
            "label": {
              "en": "Author",
              "de": "Autor",
              "fr": "Auteur",
              "it": "Autore",
              "ja": "執筆者",
              "es": "Autor",
              "pt-BR": "Autoria"
            },
            "default": {
              "en": "Author's name",
              "de": "Name des Autors",
              "fr": "Nom de l'auteur",
              "it": "Nome dell'autore",
              "ja": "筆者の名前",
              "es": "Nombre del autor",
              "pt-BR": "Nome do(a) autor(a)"
            }
          },
          {
            "type": "text",
            "id": "title",
            "label": {
              "en": "Title"
            },
            "default": {
              "en": "Interior Design"
            }
          },
          {
            "type": "image_picker",
            "id": "image",
            "label": "Image"
          }
        ]
      }
    ],
      "presets": [
      {
      "name": "Product Testimonials",
      "category": "Product"
      }
    ]
  }
{% endschema %}
